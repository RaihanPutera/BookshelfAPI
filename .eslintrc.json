{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "indent": "off",
        "eol-last": "off",
        "space-before-blocks": "off",
        "no-trailing-spaces": "off",
        "implicit-arrow-linebreak": "off",
        "no-else-return": "off"
    }
}
